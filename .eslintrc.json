{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "mocha": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "global-require": 0,
    "no-underscore-dangle": 0,
    "prefer-destructuring": ["error", { "object": true, "array": false }]
  }
}
